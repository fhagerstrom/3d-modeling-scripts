spotLight -i 1.3 -rs on -name "keyLight";
move 5 9 7;
rotate 45 30 0;

directionalLight -i 0.8 -rs off -name "fillLight";
move -3 8 6;
rotate -21 -16 7;

pointLight -i 1.8 -rs off -name "backLight";
move 0 4 -5;

polyPlane -w 25 -h 25 -name "groundPlane";
select "keyLight" "fillLight" "backLight" "groundPlane";
group -name "StandardLight"
